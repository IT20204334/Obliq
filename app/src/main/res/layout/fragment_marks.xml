<?xml version="1.0" encoding="UTF-8"?>

    <androidx.constraintlayout.widget.ConstraintLayout
    android:background="@drawable/obliq_bg"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:id="@+id/marksFragment" xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:android="http://schemas.android.com/apk/res/android">

    <Button android:layout_height="72dp"
        android:layout_width="270dp"
        android:id="@+id/toViewMarksBtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"

        android:textSize="20sp"
        android:textColor="@color/white"
        android:text="RESULTS SHEET>>"
        android:layout_marginBottom="148dp"
        android:layout_marginRight="144dp"
        android:layout_marginEnd="144dp"
        android:layout_marginLeft="144dp"
        android:layout_marginStart="144dp"/>

    <ImageView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/imageView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/obliq_marks"
        app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="61dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>